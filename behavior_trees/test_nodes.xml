<root main_tree_to_execute="MainTree" BTCPP_format="4">
  <include path="tree_nodes_model.xml" />
  
  <BehaviorTree ID="MainTree">
    <Sequence name="RootSequence">
      <Action ID="StackDetection" stack_pose="{stack_pose}" />
      <Action ID="StoreCurrentJointPos" joint_out="{stored_joints}" />
      <Action ID="MoveCartesian" target_pose="{stack_pose}" ik_frame="wrist_3_link" time="2.5" />
      <Action ID="MoveEEF" z="0.2" time="2.5"/>
      <Action ID="MoveJoint" joint_values="{stored_joints}" time="2.5" />
      <Action ID="MoveJoint" joint_values="1;2;3;4;5;6" time="2.5" />
    </Sequence>
  </BehaviorTree>

</root>

